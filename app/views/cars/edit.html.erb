<%= form_for(@cars) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.text_field :name%>
    <%= f.text_field :namsx %>
    <%= f.text_field :color %>
    <%= f.text_field :price %>
  </div>
  <%= f.submit "Update", class: "btn btn-primary" %>
<% end %>