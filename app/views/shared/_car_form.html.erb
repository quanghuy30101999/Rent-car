<%= form_for(@car) do |f| %>
  <%= render "shared/error_messages", object: f.object %>
  <div class="field">
  <label> Tên </label>
    <%= f.text_field :name %>
    <label> Năm </label>
    <%= f.text_field :year %>
    <label> Màu </label>
    <%= f.text_field :color %>
    <label> Gía </label>
    <%= f.text_field :price %>
  </div>
  <%= f.submit "Post", class: "btn btn-primary" %>
<% end %>