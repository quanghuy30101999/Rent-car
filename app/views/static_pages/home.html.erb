<div class="center jumbotron">
  <h1>Welcome to the Rent Car</h1>
</div>
<div class="container">
    <div class="col-md-4">
      <nav class="navbar navbar-light bg-light justify-content-between">
      <%= form_tag(search_path, :method => "get", class: "form-inline") do %>
            <div class = "col-md-6">  
            <p><%= search_field_tag :search, params[:search], placeholder: "Search", class: "form-control mr-sm-2" %></p>
            <label>Available From</label>
            <%= date_field_tag "Available_From", :available_from %>
            <label>Available To</label>
            <%= date_field_tag "Available_To", :available_to %>
            <%= button_tag "Search", :class => "btn btn-outline-success my-2 my-sm-0" %>
          <% end %>
      </nav>
      </div>
    </div>
    <div class="col-md-12">
      <%= render "shared/feed" %>
    </div>
</div>

